{
  "name": "personal-site",
  "version": "2.0.0",
  "description": "www.davidcsally.com",
  "private": true,
  "scripts": {
    "dev": "yarn start",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "react-snap",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "serve-build": "serve -s build",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidcsally/davidcsally.github.io.git"
  },
  "author": "David Sally",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/davidcsally/davidcsally.github.io/issues"
  },
  "homepage": "https://www.davidcsally.com",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "dependencies": {
    "border-box.css": "^1.0.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-react": "^7.21.5",
    "framer-motion": "^2.9.1",
    "gh-pages": "^3.1.0",
    "normalize.css": "^8.0.1",
    "react": "^16.14.0",
    "react-dom": "^16.13.1",
    "react-modal": "^3.11.2",
    "react-scripts": "^3.4.3",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/addons": "^6.1.9",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.26",
    "@types/jest": "^26.0.14",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-modal": "^3.10.6",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "fork-ts-checker-webpack-plugin": "^5.2.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-snap": "^1.23.0",
    "serve": "^11.3.2",
    "ts-loader": "^8.0.5",
    "typescript": "^3.9.7",
    "typescript-eslint": "^0.0.1-alpha.0"
  }
}
